	{{ page.help }}
	<section class="section" data-scroll="completed">
		<div class="container full">
			<h1 class="center"><strong>WHAT IS PROTEIN?</strong></h1>
		</div>
		  </section>
	<section class="section rightBg">
		<div class="container">{{ node.body.value|raw }}</div>
	</section>
	
	<section class="section proteinSourcesSec leftBg">
		<div class="container">
			<div class="share">
				<span>Share on</span>
				<a class="fb" href="javascript:;"></a>
				<a class="twitter" href="javascript:;"></a>
		</div>
			<div class="compoTitle">
				<h2>Sources of Protein</h2>
				<p>Protein is everywhere you look. We narrow the popular sources for proteins for all diet types:</p>
			</div>
			<div class="sourceList">
				<ul>
					{% for item in node.field_content %}
						<li data-scroll="active">
							<div class="imgBox svgBox" data-svg="/{{ directory }}/images/sources-of-protein/01_Yellow.svg">
								<img src="{{ file_url(item.entity.field_content_image.entity.uri.value) }}" alt="{{ item.entity.field_title.value }}">
							</div>
							<h3>{{ item.entity.field_title.value }}</h3>
							{{ item.entity.field_content_body.value|raw }}
						</li>
					{% endfor %}																							
				</ul>
			</div>
		</div>
  </section>
	<section class="section thePowerOfProteinSec rightBg" data-scroll="">
    <div class="container">
			{{ page.know_protein }}
    </div>
  </section>
	
	<footer id="footer">
		{% if page.footer %}
			<div class="ocean">
				<div class="wave"></div>
				<div class="wave"></div>
			</div>
			{{ page.footer }}
		{% endif %}
	</footer>
	<div class="searchLayer">
		<span class="close"></span>
		<div class="formBox">
		<form action="/search" method="post" id="askForm">
			<div class="fieldBox wid100 requiredField">
				<div class="inputBox">
					<input name="search" id="name" type="text" placeholder="What are you looking for?" />
				</div>
			</div>
			<button type="submit" class="searchBtn"></button>
		</form>
		</div>
	</div>
	<div class="lBoxMaster">
		<div class="overlay"></div>
		<div class="lBox">
			<span class="close">X</span>
			<div class="container">
		    <div class="iframe">
		      <h3 style="display:none;"></h3>
			    <iframe width="100%" height="100%" src=""></iframe>
		    </div>
				<div class="lContent"></div>
			</div>
		</div>
	</div>
</div>
