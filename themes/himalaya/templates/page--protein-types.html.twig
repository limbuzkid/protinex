<div id="page-wrapper" class="wrapper sourcesOfProteinTemplate">
  <header>
		<div class="container">
			<div class="hamburger"><span></span><span></span><span></span></div>
			{{ page.header }}
			<!--<a class="logo fl" href="/"><img src="{{ logopath }}" /></a>-->
