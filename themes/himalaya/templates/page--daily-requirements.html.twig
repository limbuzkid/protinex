	<section class="section titleSec rightBg" data-scroll="completed">
		<div class="container full">{{ node.body.value|raw }}</div>
	</section>
	
	{% set cnt = 1 %}
	{% set first = true %}
	
	<section class="section proteinSourcesSec">
		<div class="container">
			<div class="compoTitle"><h3>Daily Protein Requirements vary on the following factors:</h3></div>
			<div class="sourceList reqFactorList">
				<ul>
					{% for item in node.field_content %}
						{% if cnt > 4 %}
							{% set cnt = 1 %}
						{% endif %}
						
						{% if first %}
							{% set active = 'active' %}
							{% set first = false %}
						{% else %}
							{% set active = '' %}
						{% endif %}
						<li data-scroll="{{ active }}">
							<div class="imgBox svgBox" data-svg="/{{ directory }}/images/Recipes{{ cnt }}.svg">
								<img src="{{ file_url(item.entity.field_content_image.entity.uri.value) }}" alt="{{ item.entity.field_title.value }}">
							</div>
							<h3>{{ item.entity.field_title.value }}</h3>
							<div class="content">{{ item.entity.field_content_body.value|raw }}</div>
						</li>
						{% set cnt = cnt + 1 %}
					{% endfor %}
				</ul>
			</div>
		</div>
	</section>
	
