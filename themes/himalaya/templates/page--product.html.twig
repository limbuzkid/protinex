{#
/**
 * @file
 * Himalaya's theme implementation to display a single page.
 *
 */
#}


<div id="page-wrapper" class="wrapper productDetailsTemplate">
  <header>
		<div class="container">
			<div class="hamburger"><span></span><span></span><span></span></div>
			{{ page.header }}
			<!--<a class="logo fl" href="/"><img src="{{ logopath }}" /></a>-->
			{{ page.logo }}
			<nav class="mainNavs">
				<a class="searchBtn" href="javascript:;"></a>
				{{ page.primary_menu }}
			</nav>
		</div>
	</header>
	<section class="breadcrumbs">
		{{ page.breadcrumb }}
	</section>
	<section class="buyBtnOnBanner">
		<p>Protinex Original</p>
		<a class="btn yellow" href="{{ term.field_amazon_link.value }}" target="_blank">Buy</a>
	</section>
	<section class="section bannerSection">
		<div class="tempBanner">
			<img src="{{ file_url(term.field_product_banner.entity.uri.value) }}" alt="banner" data-banner="{{ file_url(term.field_product_banner.entity.uri.value) }}" data-banner-mobile="{{ file_url(term.field_mobile_banner.entity.uri.value) }}">
		</div>
		<div class="bannerWave">
			<svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
				 width="5760px" height="256.104px" viewBox="0 0 5760 256.104" enable-background="new 0 0 5760 256.104" xml:space="preserve"  preserveAspectRatio="none">
				<path class="topFill"  fill="#efa23e" d="M0,114.795c121.796,28,273.796,66.667,491.129,5.333c264.626-74.681,416.667-90.667,627.333-18
					c273.396,94.304,463.596,35.19,616.667-21.333c293.86-108.512,525.837-106.824,806.446-0.348
					c598.698,227.174,758.009,122.678,939.078,42.444c167-74,393.488-172.591,645.476-81.096c121.144,43.987,320.049,184.506,577,179
					c280-6,625.486-202,1056.871-106v141.309H0V114.795z"/>
				<path class="bottomFill" fill="#f2c146" d="M0,157.177c164.327,59.618,342.795,45.618,557.796-39.049c242.612-95.54,438.837-53.94,611.765,13.468
					c269.455,105.035,524.404,17.174,682.693-30.604c264.211-79.748,504.601-41.485,653.447-2.101
					c359.236,95.05,524.974,205.49,830.199,86.051c316.537-123.867,609.317-210.178,931.229-62.146
					c274,126,399.884,106.925,525.109,98.784c177.056-11.51,599.21-178.118,967.762-64.402v98.927H0V157.177z"/>
				<path class="whiteFill" fill="#FFFFFF" d="M0,214.573c274.994,80.812,403.129-4.777,612.129-95.777c275.455-119.935,463.657-0.523,642.568,57.871
					c279.025,91.07,556.754-3.617,665.275-30.734c188.249-47.041,464.127-43.249,665.648,2.184
					c255.703,57.646,470.738,151.178,869.508,18.68c259.875-86.348,536-97.769,804-16c251.557,76.752,435.64,119.716,722.648,47.996
					c348.1-86.985,642.838-35.496,778.223,15.781v41.531H0V214.573z"/>
			</svg>
		</div>
	</section>
	{{ page.help }}
	<section class="section" data-scroll="active">
    <div class="container">
      <div class="columnMaster">
        <div class="column wid60 contentSec contentTitleSec fr">
          <div class="topSec">
            <h1 class="redText">{{ term.name.value }}</h1>
						{{ term.description.value|raw }}
						<div class="share social">
							<span>Share on</span>
							<a class="fb" href="javascript:;"></a>
							<a class="twitter" href="javascript:;"></a>
						</div>
          </div>
        </div>
				
        <div class="column wid40">
          <div class="availFlavLeft">
            <div id="productCarousel2" class="productCarousel owl3D">
							<div class="productItem" data-productName="{{ term.field_flavour.value }}" data-productLink="{{ term.field_amazon_link.value }}" data-colorType="{{ term.field_color.value }}">
								<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="553px"
									height="461px" viewBox="0 0 553 461" enable-background="new 0 0 553 461" xml:space="preserve">
										<g class="productLayer">
											<image overflow="visible" width="291" height="374" class="productLayer_Image" xlink:href="{{ file_url(term.field_menu_image.entity.uri.value) }}"  transform="matrix(1 0 0 1 129 0)"></image>
										</g>
										<!--<g class="gradientLayer">
											<image overflow="visible" width="266" height="87" class="gradientLayer_Image" xlink:href="/{{ directory }}/images/gradient.png"  transform="matrix(1 0 0 1 149 287)"></image>
										</g>-->
										<g class="splashLayer">
											<image overflow="visible" width="553" height="338" class="splashLayer_Image" xlink:href="{{ file_url(term.field_splash_image.entity.uri.value) }}"  transform="matrix(1 0 0 1 0 123)"></image>
										</g>
								</svg>
							</div>
							{{ page.product_flavours }}
							<div class="">
								<h3>Product Highlight</h3>
								<div class="productHighlight">
									<ul>
										{% for item in term.field_product_highlight %}
											<li>
												<div class="imgSec"><img src="{{ file_url(item.entity.field_menu_image.entity.uri.value) }}" alt="{{ item.entity.name.value }}"></div>
												<p>{{ item.entity.name.value }}</p>
											</li>
										{% endfor %}
									</ul>
								</div>
							</div>
							{% if term.field_brand_tvcs is not empty  %}
								<h3>Our Brand TVCs</h3>
								<div class="ourBrandCarousel">
									{% for vid in term.field_brand_tvcs %}
										{% if vid.value  %}
											<div class="item">
												<div class="imgSec">
													<img src="https://img.youtube.com/vi/{{ vid.value }}/hqdefault.jpg" alt="">
													<a class="videoIcon viewLBox" href="javascript:;" data-video="https://www.youtube.com/embed/{{ vid.value }}?autoplay=1"></a>
												</div>
											</div>
										{% endif %}
									{% endfor%}
								</div>
							{% endif %}
          </div>
        </div>
			</div>
    </div>
  </section>
	<section class="section" data-scroll="">
		<div class="container">
			<div class="detailsSec">
				<div class="columnMaster">
					<div class="column wid70">
						<div class="tabSec">
							<div class="tabContent">
								<div class="col detail">
									<h3 class="accTitle">Details</h3>
									<div class="acctDiv">{{ term.field_details.value|raw }}</div>
								</div>
								<div class="col">
									<h3 class="accTitle">Ingredients</h3>
									<div class="acctDiv">{{ term.field_ingredients.value|raw }}</div>
								</div>
								<div class="col">
									<h3 class="accTitle">Dosage</h3>
									<div class="acctDiv">{{ term.field_dosage.value|raw }}</div>
								</div>
								<div class="col">
									<h3 class="accTitle">How to Use</h3>
									<div class="acctDiv">{{ term.field_how_to_use.value|raw }}</div>
								</div>
							</div>
						</div>
					</div>      
					<aside class="column wid30">
						<div class="nutritionalCom yellowContent">
							<h2>Nutritional <span>COMPARISON</span></h2>
							<h3>(Approx. Values)</h3>
							<p>Check out the amazing and essential nutrients that are packed in your favorite Protinex.</p>
							<a class="btn viewLBox" href="javascript:;">View chart</a>
							<div class="hiddenContent">
								<div class="nutritionInfoTable">
									<h2><strong>Nutritional</strong> Information</h2>
									<p>(Approx. Values per 100g)</p>
									<h3>Find out what your favorite Protinex packs in terms of nutrition!</h3>
									{{ term.field_nutritional_info_chart.value|raw }}
									<p class="note">Nutritional information displayed for other brands is based on information declared on the pack label</p>
								</div>
							</div>
							<div class="imgSec"><img src="/themes/himalaya/images/nutritional-com.png" alt=""></div>
						</div>
					</aside>
        </div>
      </div>
    </div>
  </section>
    
	<section class="section" data-scroll="">
		<div class="container">
			<div class="featuredRecipesBttm">
				<div class="topSec">
					<div class="titleSec">
						<h2>#ChillWithProtein</h2>
						<p>Check out these awesome recipes that you can make using Protinex:</p>
					</div>
				</div>
				{{ page.recipes }}
				<a class="btn gray recipesAllBtn" href="/recipes">View All</a>

			</div>
		</div>
	</section>
	
  <footer>
		<div class="ocean">
			<div class="wave"></div>
			<div class="wave"></div>
		</div>
		{{ page.footer }}
	</footer>
	<div class="searchLayer">
		<span class="close"></span>
		<div class="formBox">
		<form action="/search" method="post" id="askForm">
			<div class="fieldBox wid100 requiredField">
				<div class="inputBox">
					<input name="search" id="name" type="text" placeholder="What are you looking for?" />
				</div>
			</div>
			<button type="submit" class="searchBtn"></button>
		</form>
		</div>
	</div>
	<div class="lBoxMaster">
		<div class="overlay"></div>
		<div class="lBox">
			<span class="close">X</span>
			<div class="container">
		    <div class="iframe">
		      <h3 style="display:none;"></h3>
			    <iframe width="100%" height="100%" src=""></iframe>
		    </div>
				<div class="lContent"></div>
			</div>
		</div>
	</div>
</div>


