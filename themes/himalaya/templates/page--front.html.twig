	<section class="section knowProtien leftBg" data-scroll="active">
		<div class="container">
			<div class="box">
				{{ page.know_protein }}
			</div>
		</div>
	</section>
	
	<section class="section ourProducts" data-scroll="">
		<div class="container full">
			<h2 class="center">Our Range of <strong>Products</strong></h2>
			<p>There's a Protinex for every member of the family. <br>Find out what's right for you.</p>
			{{ page.home_product }} 
			<div class="productNameLink">
				<h2>Protinex</h2>
				<a href="javascript:;" target="_blank" href="https://www.google.com" class="btn">Read More</a>
			</div>
			<div id="slider"></div>
		</div>
	</section>
	<section class="section myths rightBg" data-scroll="completed">
		<div class="container">
			<div class="columnMaster vCenter fRight">
				{{ page.home_protein_myths }}
			</div>
		</div>
	</section>
	
	
	<section class="section calculator leftBg" data-scroll="">
		<div class="container">
			<div class="columnMaster vCenter">
				{{ page.home_protein_count }}
			</div>
		</div>
	</section>
	
	<section class="section recipes rightBg" data-scroll="">
		<div class="container">
			<div class="columnMaster vCenter">
				{{ page.home_meets_taste }}
			</div>
		</div>
	</section>
	
	<section class="section helpSection" data-scroll="">
		<div class="container">
			<div class="columnMaster">
				<div id="askExpert" class="column wid40">
					<h2 class="blueText">Ask the <strong>experts</strong></h2>
					<p>We'd love to talk protein with you.</p>
					<p>So do send us your questions.</p>
					<div class="formBox">
						<div class="fieldBox wid100 requiredField">
							<div class="inputBox">
								<input name="Name" id="name" data-label="true" type="text" placeholder="Name" />
							</div>
						</div>
						<div class="fieldBox wid100 requiredField">
							<div class="inputBox">
								<input name="Email" id="email" data-label="true" type="text" placeholder="Email" />
							</div>
						</div>
						<div class="fieldBox wid100 requiredField">
							<div class="textareaBox">
								<textarea id="query" data-label="true" name="query" rows="5" cols="60" placeholder="Query"></textarea>
							</div>
						</div>
						<div class="fieldBox wid100">
							<input value="submit" class="btn blue center fr askSbmt" type="button"/>
						</div>
						<input value="" class="token" type="hidden"/>
					</div>
				</div>
				<div id="blog" class="column wid60">
					<div class="blogBox">	
						<div class="imgBox">							<img src="/{{ directory }}/images/blob-green-fill.png" alt="">
							<img src="/{{ directory }}/images/blog.png" alt="Blog">
						</div>
						<h2 class="greenText"><strong>BLOG</strong></h2>
						{{ page.sidebar_right }}
						<a class="btn green center fr" href="/blog">Browse</a>
					</div>			
				</div>
			</div>
		</div>
	</section>
	
	<!--<section class="section testimonial leftBg" data-scroll="">
		<div class="container">
			<div class="columnMaster">
				<div class="column wid100 tCenter">
					<div class="testimonialContent">
						<div class="testimonialData">
							<h2><strong>We love you right back!</strong></h2>
							<p>Over 57 years, we've been bringing healthy smiles to millions of families across the globe. <br>Here's what some of them have to say.</p>
							{{ page.testimonial }}
						</div>
					</div>
					<a href="/testimonials" class="btn yellow">View All</a>
				</div>
			</div>
		</div>
	</section>-->
	<footer>
		<div class="ocean">
			<div class="wave"></div>
			<div class="wave"></div>
		</div>
		{{ page.footer }}
	</footer>
	

	
	<div class="searchLayer">
		<span class="close"></span>
		<div class="formBox">
			<form action="/search" method="post" id="askForm">
				<div class="fieldBox wid100 requiredField">
					<div class="inputBox">
						<input name="search" id="name" type="text" placeholder="What are you looking for?" />
					</div>
				</div>
				<button type="submit" class="searchBtn"></button>
			</form>
		</div>
	</div>
	<div class="lBoxMaster">
		<div class="overlay"></div>
		<div class="lBox">
			<span class="close">X</span>
			<div class="container">
		    <div class="iframe">
		      <h3 style="display:none;"></h3>
			    <iframe width="100%" height="100%" src=""></iframe>
		    </div>
				<div class="lContent"></div>
			</div>
		</div>
	</div>
	<!-- Lightbox Start-->
    <div id="overlay"></div>
    <div id="lightboxWrap">
        <a href="javascript:" class="closeBtn"><img src="themes/himalaya/images/close.png" alt="Close" title="Close"></a>
        <div class="lightbox">
            <img src="themes/himalaya/images/Light-box.gif" alt="" class="desktop">
            <img src="themes/himalaya/images/Light-box-mobile.gif" alt="" class="device">
        </div>
        <a href="javascript:" class="proceedBtn"><img src="themes/himalaya/images/proceedBtn.png" alt="proceed to website" title="proceed to website"></a>
    </div>
    <!-- Lightbox End-->
</div>